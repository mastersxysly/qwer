package leetcode0_50;
/**
 * 
 * @author xx
 *
 */
public class T8 {
	public int myAntoi(String str) {
		str = str.trim();
		int flag = 0;
		long res = 0;
		int i = 0;
		if("-".equals(str.charAt(0))) {
			flag = 1;
			i++;
		}
		while(i < str.length()-1) {
			if(str.charAt(i) <= 9 && str.charAt(i) >= 0) {
				res = res*10+ Integer.valueOf(str.charAt(i));
				i++;
			}else {
				break;
			}
			
		}
		return 0;
	}
	public static void main(String[] args) {
		System.out.println(new T8().myAntoi("   -123 rrr"));
	}
}
