package leetcode0_50;
/**
 * 有符号整数反转
 * 防止溢出
 * @author xx
 *
 */
public class T7 {
	public int reverse(int x) {
		int ans = 0;
		while(x != 0) {
			int pop = 0;
			pop  = x % 10;
			ans = ans * 10 + pop;	
			x /= 10;
		}
		return ans;
	}
	public static void main(String[] args) {
		System.out.print(new T7().reverse(123));
	}
}
